@model IEnumerable<Model.Goods>
@{
    Layout = null;
}

<!DOCTYPE html>
<style>
     .right-content{
    width:60%;
    height:190px;
    margin-left:40%;
}
.right-content .right-content-box{
    width:100%;
    height:170px;
    margin-top:0px;
}
.right-content .right-content-box li{
    height:170px;
    width:22%;
    margin-left:2.3%;
    float:left;
    border:1px solid #dcdcdc;
    cursor:pointer;
    text-align:center;
}
.right-content .right-content-box li img{
    height:145px;
    width:100%;
    margin-top:0px;
    background-size:cover;
}
.right-content .right-content-box li a{
    height:15px;
    width:100%;
    font-size:11px;
    text-decoration:none;
    margin-top:0px;
}
.right-content .right-content-box li a:hover
{
    color:#339933;
}
</style>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ShopMallShopsImage</title>
</head>
<body>
    <div class="right-content">
        <ul class="right-content-box">
           @foreach(var good in Model)
            {
                <li>
                    <img src="@good.GoodsPhoto" />
                    <a>@good.GoodsName</a>
                </li>
            }
        </ul>
    </div>

</body>
</html>
